# Hedera Network Configuration
# Copy this file to .env and fill in your actual values

# Testnet Configuration (for development)
REACT_APP_HEDERA_TESTNET_ACCOUNT_ID=0.0.YOUR_TESTNET_ACCOUNT_ID
REACT_APP_HEDERA_TESTNET_PRIVATE_KEY=YOUR_TESTNET_PRIVATE_KEY

# Mainnet Configuration (for production)
REACT_APP_HEDERA_MAINNET_ACCOUNT_ID=0.0.YOUR_MAINNET_ACCOUNT_ID
REACT_APP_HEDERA_MAINNET_PRIVATE_KEY=YOUR_MAINNET_PRIVATE_KEY

# Network Environment
NODE_ENV=development

# Supabase Configuration
# Get these values from your Supabase project settings
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here